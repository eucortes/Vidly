@model Vidly.ViewModels.MoviesIndexViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<div class="container  col-lg-12">
<table class="table-striped ">
    <thead>
        <tr><th>Movies</th></tr>
    </thead>
    @foreach (var movie in Model.Movies)
    {
        <tbody>
    <tr>
        <td>
            @movie.Name
        </td>
    </tr>
        </tbody>

    }

</table>
    </div>
